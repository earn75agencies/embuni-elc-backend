{
  "name": "elp-frontend",
  "version": "1.0.0",
  "description": "Frontend for University of Embu Equity Leaders Program",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:production": "npm run lint && npm run typecheck && vite build",
    "preview": "vite preview",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run",
    "test:ci": "vitest run --coverage && start-server-and-test preview http://localhost:4173 \"cypress run\"",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "i18next": "^25.7.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.555.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-i18next": "^16.3.5",
    "react-router-dom": "^7.10.1",
    "react-slick": "^0.31.0",
    "react-toastify": "^11.0.5",
    "recharts": "^3.5.1",
    "slick-carousel": "^1.8.1",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^4.1.17",
    "terser": "^5.44.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.15",
    "autoprefixer": "^10.4.22",
    "cypress": "^15.7.1",
    "eslint": "^9.39.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.2.0",
    "msw": "^2.12.4",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "start-server-and-test": "^2.1.3",
    "vite": "^7.2.6",
    "vitest": "^4.0.15"
  }
}
