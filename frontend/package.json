{
  "name": "elp-frontend",
  "version": "1.0.0",
  "description": "Frontend for University of Embu Equity Leaders Program",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:production": "npm run lint && npm run typecheck && vite build",
    "preview": "vite preview",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run",
    "test:ci": "vitest run --coverage && start-server-and-test preview http://localhost:4173 \"cypress run\"",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "i18next": "^23.7.0",
    "i18next-browser-languagedetector": "^7.2.0",
    "lucide-react": "^0.309.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.5.0",
    "react-router-dom": "^6.20.0",
    "react-slick": "^0.30.0",
    "react-toastify": "^9.1.0",
    "recharts": "^2.10.3",
    "slick-carousel": "^1.8.1",
    "socket.io-client": "^4.5.4",
    "tailwindcss": "^3.4.0",
    "terser": "^5.44.1",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^4.0.13",
    "autoprefixer": "^10.4.0",
    "cypress": "^13.6.0",
    "eslint": "^8.55.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^22.1.0",
    "msw": "^2.12.3",
    "postcss": "^8.4.0",
    "prettier": "^3.1.0",
    "start-server-and-test": "^2.0.0",
    "vite": "^7.2.2",
    "vitest": "^4.0.13"
  }
}
