    "winston": "^3.11.0",
    "xss-clean": "^0.1.3"
  },
  "devDependencies": {
    "axios": "^1.13.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "supertest": "^6.3.3"
