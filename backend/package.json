    "compression": "^1.7.4",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.1",
    "express-session": "^1.17.3",
    "express-validator": "^7.3.0",
    "handlebars": "^4.7.8",
    "he": "^1.2.0",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.3.2",
    "isomorphic-dompurify": "^2.33.0",
    "jsonwebtoken": "^9.0.2",
